<div id="distribution" v-cloak>
  <div class="container">
    <h1 class="text-center" style="margin-top: 30px;">岗位设置</h1>
    <h6 class="text-center statistic-link"><a href="/admin/dashboard">上传Excel表</a></h6>
    <table class="table clickable table-hover large" v-if="true">
      <thead>
        <tr>
          <th>序号</th>
          <th>岗位名称</th>
          <th>人数</th>
          <th>操作</th>
        </tr>
      </thead>
      <tbody>
        <tr v-for="data of posts" >
          <td>{{data.id}}</td>
          <td>{{data.name}}</td>
          <td>{{data.member}}</td>
          <td>
            <button type="button" class="btn btn-primary" >
              <i class="fa fa-edit opp-icon" aria-hidden="true"></i>
            </button>
            <button type="button" class="btn btn-danger" >
              <i class="fa fa-trash-o opp-icon" aria-hidden="true"></i>
            </button>
          </td>
        </tr>
      </tbody>
    </table>
  </div>
</div>
<%- /* Expose locals as `window.SAILS_LOCALS` :: */ exposeLocalsToBrowser() %>
